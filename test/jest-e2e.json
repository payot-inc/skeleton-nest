{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/../src/$1",
    "@app/bizm/(.*)": "<rootDir>/../libs/bizm/src/$1",
    "@app/bizm": "<rootDir>/../libs/bizm/src"
  }
}
